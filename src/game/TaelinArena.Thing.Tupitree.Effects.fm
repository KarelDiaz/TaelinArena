import TaelinArena.Game

tupitree_entangling_fun(self: Thing) : Thing
  let self = animate(self, 0, TUPITREE_ENTANGLING_FX_000, 11, 22)
  let self = self <= thing(die = if tik === 22 then true else false)
  self

tupitree_natures_revenge_fun(self: Thing) : Thing
  let self = animate(self, 0, TUPITREE_NATURE_REVENGE_ALL_000, 6, 18)
  let self = self <= thing(die = if tik === 18 then true else false)
  self