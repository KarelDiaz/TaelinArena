import TaelinArena.Game
import TaelinArena.Models
import TaelinArena.Heroes.Mikegator
import TaelinArena.Heroes.Shao
import TaelinArena.Heroes.Min
import TaelinArena.Heroes.Zoio
import TaelinArena.Heroes.Teichi

enum
| MIKEGATOR_HERO
| SHAO_HERO
| MIN_HERO
| ZOIO_HERO
| TEICHI_HERO

get_hero_by_hid(hid: HeroId) : Hero
  switch hid
  |MIKEGATOR_HERO mikegator_hero
  |SHAO_HERO shao_hero
  |MIN_HERO min_hero
  |ZOIO_HERO zoio_hero
  |TEICHI_HERO teichi_hero
  else mikegator_hero
